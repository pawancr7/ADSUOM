#include <iostream>
using namespace std;

int main()
{
    int a[20], i, n;
    cin >> n;
    for (i = 0; i <= n - 1; cin >> a[i], i++)
        ;
    for (i = 0; i <= n - 1; cout << a[i], i++)
        ;
    cout << endl;
    cout << "Reverse " << endl;
    for (i = n - 1; i >= 0; cout << a[i], i--)
        ;
    return 0;
}
