#include <iostream>
using namespace std;

int main()
{
    int a[20], i, n, min = 0, max = 0;
    cout << "Enter array size " << endl;
    cin >> n;
    for (i = 0; i <= n - 1; cin >> a[i], i++)
        ;
    max = a[0];
    for (i = 0; i <= n - 1; i++)
    {
        if (a[i] > max)
            max = a[i];
    }
    min = a[0];
    for (i = 0; i <= n - 1; i++)
    {
        if (a[i] < min)
            min = a[i];
    }
    cout << "Max of array " << max << endl;
    cout << "Min of array " << min;

    return 0;
}
